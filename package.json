{
  "name": "wisp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "start": "vue-cli-service serve",
    "analyze": "vue-cli-service build --report && open dist/report.html",
    "test:unit": "vue-cli-service test:unit",
    "test": "jest"
  },
  "dependencies": {
    "@baoshishu/vue-editor": "^0.3.0",
    "@baoshishu/vue-query": "^0.1.6",
    "@sentry/browser": "^5.22.3",
    "@sentry/integrations": "^5.22.3",
    "@sentry/tracing": "^5.22.3",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-service": "^4.5.4",
    "@vue/composition-api": "^0.6.7",
    "babel-eslint": "^10.1.0",
    "date-fns": "^2.16.1",
    "element-ui": "^2.13.2",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-vue": "^6.2.2",
    "graphql": "^15.3.0",
    "prosemirror-utils": "^0.9.6",
    "tiptap": "^1.29.6",
    "tiptap-extensions": "^1.32.6",
    "v-tooltip": "^2.0.3",
    "vue": "^2.6.12",
    "vue-router": "^3.4.3",
    "vue-slicksort": "^1.1.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.1",
    "babel-jest": "^24.8.0",
    "graphql-tag": "^2.10.3",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^1.7.6",
    "vue-loader": "^15.9.1",
    "vue-svg-loader": "0.11.0",
    "vue-template-compiler": "^2.6.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "ignore": [
      "tailwind.js"
    ],
    "linters": {
      "*.{js,vue,json,graphql,md}": [
        "prettier --write",
        "git add"
      ],
      "*.{js,vue}": [
        "eslint --fix",
        "git add"
      ]
    }
  }
}
